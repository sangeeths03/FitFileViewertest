  "build": {
    "icon": "icons/favicon.ico",
    "appId": "com.example.fitfileviewer",
    "artifactName": "Fit-File-Viewer-${platform}-${arch}-${version}.${ext}",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "Nick2bad4u",
        "repo": "FitFileViewer"
      }
    ],
    "win": {
      "icon": "icons/favicon-256x256.ico",
